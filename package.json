{
  "name": "mantis",
  "version": "1.0.0",
  "description": "nft feed",
  "main": "index.js",
  "scripts": {
    "lint": "eslint",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "test": "cross-env NODE_ENV=test nyc --reporter=text --reporter=html mocha src/test/*.js --timeout 10000 -c --require @babel/register --exit",
    "start": "node dist/index.js",
    "dev": "nodemon --exec babel-node ./src/index.js",
    "build": "babel src -d dist",
    "migrate": "sequelize db:migrate ",
    "migrate:reset": "sequelize db:migrate:undo:all",
    "seed": "sequelize db:seed:all",
    "migrate:seed": "npm run migrate && npm run seed",
    "pretest": "cross-env NODE_ENV=test npm run migrate:reset && cross-env NODE_ENV=test npm run migrate && cross-env NODE_ENV=test npm run seed",
    "prestart": "npm run migrate",
    "migrate:dev": "npm run migrate && nodemon --exec babel-node ./src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dchima/mantis.git"
  },
  "keywords": [
    "exercise",
    "authentications"
  ],
  "author": "Chima Nnadika",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dchima/mantis/issues"
  },
  "homepage": "https://github.com/dchima/mantis#readme",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@sendgrid/mail": "^7.6.0",
    "@types/hapi__joi": "^17.1.7",
    "axios": "^0.24.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "ejs": "^3.1.6",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "express-jwt": "^6.1.0",
    "express-session": "^1.17.2",
    "joi": "^17.1.1",
    "jsonwebtoken": "^8.5.1",
    "method-override": "^3.0.0",
    "methods": "^1.1.2",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "passport": "^0.5.1",
    "passport-google-oauth": "^2.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.3",
    "request": "^2.88.2",
    "request-ip": "^2.1.3",
    "sequelize": "^6.12.0-beta.3",
    "sequelize-cli": "^6.3.0",
    "socket.io": "^4.4.0",
    "socket.io-client": "^4.4.0",
    "tedious": "^14.0.0",
    "underscore": "^1.13.1",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.5",
    "@babel/node": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/register": "^7.16.5",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "coveralls": "^3.1.1",
    "eslint": "^8.4.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.3",
    "faker": "^5.5.3",
    "mocha": "^9.1.3",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^2.0.15",
    "nyc": "^15.1.0"
  }
}
